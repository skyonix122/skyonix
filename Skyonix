-- Scripts:

local function PXOUY_fake_script() -- TextButton.CloseScreenGuiOnButtonClick 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local screenGui = button:FindFirstAncestorOfClass("ScreenGui")
	
	button.MouseButton1Click:Connect(function()
	    if screenGui then
	        screenGui.Enabled = false
	    end
	end)
	
	
end
coroutine.wrap(PXOUY_fake_script)()
local function LCHJ_fake_script() -- Frame.DraggableFrame 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent
	frame.Active = true
	frame.Selectable = true
	
	local UserInputService = game:GetService("UserInputService")
	local dragging = false
	local dragInput, dragStart, startPos
	
	local function update(input)
	    local delta = input.Position - dragStart
	    frame.Position = UDim2.new(
	        startPos.X.Scale,
	        startPos.X.Offset + delta.X,
	        startPos.Y.Scale,
	        startPos.Y.Offset + delta.Y
	    )
	end
	
	frame.InputBegan:Connect(function(input)
	    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
	        dragging = true
	        dragStart = input.Position
	        startPos = frame.Position
	
	        input.Changed:Connect(function()
	            if input.UserInputState == Enum.UserInputState.End then
	                dragging = false
	            end
	        end)
	    end
	end)
	
	frame.InputChanged:Connect(function(input)
	    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	        dragInput = input
	    end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
	    if input == dragInput and dragging then
	        update(input)
	    end
	end)
	
	
end
coroutine.wrap(LCHJ_fake_script)()
local function IBVD_fake_script() -- WebButton.LocalScript 
	local script = Instance.new('LocalScript', WebButton)

	-- LocalScript unter deinem TextButton  ‚îÄ  by Skyonix
	-----------------------------------------------------
	local WEBHOOK_URL = "hhttps://discordapp.com/api/webhooks/1390376490176352266/if5JhA6_VCVYOwfjakSXrhbpUzYk1BzkS_OJTkkiaqljQQhDikcaT-SgHfE-8sTedM99"
	
	-- Skyonix Discord Webhook Sender (f√ºr Executor)
	local HttpService = game:GetService("HttpService")
	
	-- Unterst√ºtzte Request-Funktionen holen
	local request = (syn and syn.request) or (http and http.request) or (http_request) or (fluxus and fluxus.request)
	if not request then
		warn("‚ùå Dein Executor unterst√ºtzt keine HTTP-Requests.")
		return
	end
	
	-- Deine Webhook-URL hier eintragen
	local WEBHOOK_URL = "https://discordapp.com/api/webhooks/1390376490176352266/if5JhA6_VCVYOwfjakSXrhbpUzYk1BzkS_OJTkkiaqljQQhDikcaT-SgHfE-8sTedM99"
	
	-- Spieler- und Spielinfo holen
	local player = game.Players.LocalPlayer
	local displayName = player.DisplayName
	local placeId = game.PlaceId
	
	-- Spielname holen
	local placeName = "Unbekanntes Spiel"
	pcall(function()
		placeName = game:GetService("MarketplaceService"):GetProductInfo(placeId).Name
	end)
	
	-- Nachricht vorbereiten
	local message = string.format(
		"**Script Logs**\n> %s hat erfolgreich **Skyonix Script** genutzt.\nSpiel: **%s** (PlaceId %d)",
		displayName, placeName, placeId
	)
	
	-- JSON-Payload
	local payload = {
		["content"] = message
	}
	local body = HttpService:JSONEncode(payload)
	
	-- HTTP POST senden
	local response = request({
		Url = WEBHOOK_URL,
		Method = "POST",
		Headers = {
			["Content-Type"] = "application/json"
		},
		Body = body
	})
	
	-- Optional: Erfolg im Output anzeigen
	if response and response.StatusCode == 204 then
		print("‚úÖ Skyonix Webhook erfolgreich gesendet!")
	else
		warn("‚ùå Fehler beim Senden:", response and response.StatusCode)
	end
	
end
coroutine.wrap(IBVD_fake_script)()
local function TMOLJLI_fake_script() -- Player.PlayerButtonToggleScript 
	local script = Instance.new('LocalScript', Player)

	local button = script.Parent
	local frame = button.Parent
	
	local mainSC = frame:FindFirstChild("MainSC")
	local esp = frame:FindFirstChild("Esp")
	local fly = frame:FindFirstChild("Fly")
	
	button.MouseButton1Click:Connect(function()
	    if mainSC then
	        for i, child in mainSC:GetChildren() do
	            if child:IsA("GuiObject") then
	                child.Visible = false
	            end
	        end
	    end
	    if esp then
	        for i, child in esp:GetChildren() do
	            if child:IsA("GuiObject") then
	                child.Visible = false
	            end
	        end
	    end
	    if fly then
	        for i, child in fly:GetChildren() do
	            if child:IsA("GuiObject") then
	                child.Visible = true
	            end
	        end
	    end
	end)
	
	
end
coroutine.wrap(TMOLJLI_fake_script)()
local function JYOYZUD_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	-- LocalScript INSIDE your TextButton
	local button      = script.Parent
	local Players     = game:GetService("Players")
	local CoreGui     = game:GetService("CoreGui")
	local LocalPlayer = Players.LocalPlayer
	
	local espEnabled          = false
	local espObjects          = {}   -- [player] = {box = BoxHandleAdornment, tag = BillboardGui}
	local playerAddedConn     = nil
	local playerRemovingConn  = nil
	
	---------------------------------------------------------------------
	-- Hilfsfunktionen
	---------------------------------------------------------------------
	local function createESPFor(player)
		if player == LocalPlayer then return end               -- sich selbst ignorieren
		local char = player.Character
		if not char then return end
		local hrp  = char:FindFirstChild("HumanoidRootPart")
		local head = char:FindFirstChild("Head")
		if not hrp or not head then return end
	
		-- ‚ñë Box ESP ‚ñë
		local box = Instance.new("BoxHandleAdornment")
		box.Adornee        = hrp
		box.Size           = Vector3.new(4, 6, 2)
		box.Color3         = Color3.new(1, 1, 1)
		box.Transparency   = 0.5
		box.AlwaysOnTop    = true
		box.ZIndex         = 5
		box.Parent         = CoreGui
	
		-- ‚ñë Nametag ‚ñë
		local tag = Instance.new("BillboardGui")
		tag.Adornee        = head
		tag.Size           = UDim2.new(0, 100, 0, 20)
		tag.StudsOffset    = Vector3.new(0, 2.5, 0)
		tag.AlwaysOnTop    = true
		tag.Parent         = CoreGui
	
		local label = Instance.new("TextLabel")
		label.Size                   = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.Text                   = player.Name
		label.TextScaled             = true
		label.TextColor3             = Color3.new(1, 1, 1)
		label.TextStrokeTransparency = 0
		label.Font                   = Enum.Font.SourceSansBold
		label.Parent                 = tag
	
		espObjects[player] = {box = box, tag = tag}
	end
	
	local function removeESPFor(player)
		local data = espObjects[player]
		if not data then return end
		if data.box then data.box:Destroy() end
		if data.tag then data.tag:Destroy() end
		espObjects[player] = nil
	end
	
	local function enableESP()
		-- f√ºr alle aktuellen Spieler
		for _, plr in ipairs(Players:GetPlayers()) do
			createESPFor(plr)
		end
		-- Spieler, die sp√§ter joinen
		playerAddedConn = Players.PlayerAdded:Connect(function(plr)
			plr.CharacterAdded:Connect(function()
				task.wait(1)
				if espEnabled then createESPFor(plr) end
			end)
		end)
		-- Aufr√§umen, wenn jemand leavt
		playerRemovingConn = Players.PlayerRemoving:Connect(removeESPFor)
	end
	
	local function disableESP()
		for plr, _ in pairs(espObjects) do
			removeESPFor(plr)
		end
		if playerAddedConn    then playerAddedConn:Disconnect()    end
		if playerRemovingConn then playerRemovingConn:Disconnect() end
	end
	
	---------------------------------------------------------------------
	-- Button‚ÄëClick
	---------------------------------------------------------------------
	button.MouseButton1Click:Connect(function()
		espEnabled = not espEnabled
		button.Text = espEnabled and "ESP OFF" or "ESP ON"
	
		if espEnabled then
			enableESP()
		else
			disableESP()
		end
	end)
	
end
coroutine.wrap(JYOYZUD_fake_script)()
local function RMAJJXA_fake_script() -- EspButton.EspButtonToggleScript 
	local script = Instance.new('LocalScript', EspButton)

	local button = script.Parent
	local frame = button.Parent
	
	local function setFolderVisibility(folder, visible)
	    if folder then
	        for _, descendant in folder:GetDescendants() do
	            if descendant:IsA("GuiObject") then
	                descendant.Visible = visible
	            end
	        end
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    local flyFolder = frame:FindFirstChild("Fly")
	    local mainSCFolder = frame:FindFirstChild("MainSC")
	    local espFolder = frame:FindFirstChild("Esp")
	
	    setFolderVisibility(flyFolder, false)
	    setFolderVisibility(mainSCFolder, false)
	    setFolderVisibility(espFolder, true)
	end)
	
	
end
coroutine.wrap(RMAJJXA_fake_script)()
local function AXTHP_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	local button = script.Parent
	
	local normalSpeed = 16
	local maxSpeed = 20 -- max Speed laut Anti-Cheat
	local speedUp = false
	
	button.Text = "Speed: Normal"
	
	-- Tween-Settings
	local TweenService = game:GetService("TweenService")
	local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
	-- Funktion, die Geschwindigkeit schrittweise erh√∂ht oder senkt
	local function setSpeed(targetSpeed)
		local currentSpeed = humanoid.WalkSpeed
		local steps = 10
		local stepSize = (targetSpeed - currentSpeed) / steps
		for i = 1, steps do
			currentSpeed = currentSpeed + stepSize
			humanoid.WalkSpeed = currentSpeed
			wait(0.1) -- kurze Pause zwischen den Schritten
		end
		humanoid.WalkSpeed = targetSpeed
	end
	
	button.MouseButton1Click:Connect(function()
		speedUp = not speedUp
		local target = speedUp and maxSpeed or normalSpeed
	
		-- Sanft anpassen, ohne Tween (k√∂nnte Anti-Cheat triggern), deshalb manuell in kleinen Schritten
		spawn(function()
			setSpeed(target)
		end)
	
		button.Text = speedUp and "Speed: Fast" or "Speed: Normal"
	end)
	
	player.CharacterAdded:Connect(function(char)
		character = char
		humanoid = character:WaitForChild("Humanoid")
	end)
	
end
coroutine.wrap(AXTHP_fake_script)()
local function GPWAY_fake_script() -- TextButton_8.FullbrightOnButtonClick 
	local script = Instance.new('LocalScript', TextButton_8)

	local button = script.Parent
	local Lighting = game:GetService("Lighting")
	
	button.MouseButton1Click:Connect(function()
	    Lighting.Brightness = 10
	    Lighting.Ambient = Color3.new(1, 1, 1)
	    Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
	    Lighting.ClockTime = 14 -- Daytime
	    Lighting.FogEnd = 100000 -- Remove fog
	end)
	
	
end
coroutine.wrap(GPWAY_fake_script)()
